<==> arguments
--migrate-deps --dash-is-library-private

<==> input/entrypoint.scss
@import "library";
a {
  color: $-dash;
  background: $_underscore;
}

<==> input/_library.scss
$-dash: red;
$-underscore: blue;

<==> output/entrypoint.scss
@use "library";
a {
  color: $library.dash;
  background: $library.underscore;
}

<==> output/_library.scss
$dash: red;
$underscore: blue;
